<section class="content">
  <section class="hbox">
    <section>
      <section class="vbox">
        <section class="scrollable padder-lg w-f-md" id="bjax-target">
          <a href="#" class="pull-right text-muted m-t-lg" data-toggle="class:fa-spin" >
            <i class="icon-refresh i-lg  inline" id="refresh"></i>
          </a>
          <h2 class="font-thin m-b">发现
            <span class="musicbar animate inline m-l-sm" style="width:20px;height:20px">
              <span class="bar1 a1 bg-primary lter"></span>
              <span class="bar2 a2 bg-info lt"></span>
              <span class="bar3 a3 bg-success"></span>
              <span class="bar4 a4 bg-warning dk"></span>
              <span class="bar5 a5 bg-danger dker"></span>
            </span>
          </h2>
          <div class="row row-sm" id="menulist">

          </div>
        </section>
        <footer class="footer bg-dark">
          <div id="jp_container_N">
            <div class="jp-type-playlist">
              <div id="jplayer_N" class="jp-jplayer hide"></div>
              <div class="jp-gui">
                <div class="jp-video-play hide">
                  <a class="jp-video-play-icon">播放</a>
                </div>
                <div class="jp-interface">
                  <div class="jp-controls">
                    <div>
                      <a class="jp-previous"><i class="icon-control-rewind i-lg text_logo"></i></a>
                    </div>
                    <div>
                      <a class="jp-play"><i class="icon-control-play i-2x text_logo"></i></a>
                      <a class="jp-pause hid"><i class="icon-control-pause i-2x text_logo"></i></a>
                    </div>
                    <div><a class="jp-next"><i class="icon-control-forward i-lg text_logo"></i></a></div>
                    <div class="hide"><a class="jp-stop"><i class="fa fa-stop"></i></a></div>
                    <div><a class="" data-toggle="dropdown" data-target="#playlist"><i class="icon-list text_logo"></i></a></div>
                    <div class="jp-progress hidden-xs">
                      <div class="jp-seek-bar dk" style="width:100%">
                        <div class="jp-play-bar bg-info"></div>
                        <div class="jp-title text-lt">
                          <ul>
                            <li></li>
                          </ul>
                        </div>
                      </div>
                    </div>
                    <div class="hidden-xs hidden-sm jp-current-time text-xs text-muted text_logo"></div>
                    <div class="hidden-xs hidden-sm jp-duration text-xs text-muted text_logo"></div>
                    <div class="hidden-xs hidden-sm">
                      <a class="jp-mute text_logo" title="mute"><i class="icon-volume-2 text_logo"></i></a>
                      <a class="jp-unmute hid text_logo" title="unmute"><i class="icon-volume-off text_logo"></i></a>
                    </div>
                    <div class="hidden-xs hidden-sm jp-volume">
                      <div class="jp-volume-bar dk" style="background:#4D5D6E">
                        <div class="jp-volume-bar-value lter text_logo" style="background:#778490"></div>
                      </div>
                    </div>
                    <div>
                      <a class="jp-shuffle" title="shuffle"><i class="icon-shuffle text-muted text_logo"></i></a>
                      <a class="jp-shuffle-off hid" title="shuffle off"><i class="icon-shuffle text-lt text_logo"></i></a>
                    </div>
                    <div>
                      <a class="jp-repeat" title="repeat"><i class="icon-loop text-muted text_logo"></i></a>
                      <a class="jp-repeat-off hid" title="repeat off"><i class="icon-loop text-lt text_logo"></i></a>
                    </div>
                    <div class="hide">
                      <a class="jp-full-screen" title="full screen"><i class="fa fa-expand"></i></a>
                      <a class="jp-restore-screen" title="restore screen"><i class="fa fa-compress text-lt"></i></a>
                    </div>
                  </div>
                </div>
              </div>
              <div class="jp-playlist dropup" id="playlist">
                <ul class="dropdown-menu aside-xl dker" id="musiclist">
                  <li class="list-group-item"></li>
                </ul>
